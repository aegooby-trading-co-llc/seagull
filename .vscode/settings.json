{
    "deno.enable": false,
    "deno.unstable": true,
    "deno.lint": true,
    "deno.suggest.completeFunctionCalls": true,
    "deno.cache": ".cache/",
    "deno.suggest.autoImports": true,
    "deno.path": "/usr/local/bin/deno",
    "deno.enablePaths": ["./packages/server/embedded"],

    "editor.formatOnSave": true,

    "typescript.validate.enable": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": false,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": false,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "typescript.autoClosingTags": true,
    "javascript.autoClosingTags": true,
    "typescript.format.enable": true,
    "javascript.format.enable": true,
    "typescript.format.insertSpaceAfterCommaDelimiter": true,
    "javascript.format.insertSpaceAfterCommaDelimiter": true,
    "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
    "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
    "typescript.preferences.quoteStyle": "double",
    "javascript.preferences.quoteStyle": "double",
    "typescript.format.semicolons": "insert",
    "javascript.format.semicolons": "insert",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "typescript.format.insertSpaceAfterConstructor": false,
    "javascript.format.insertSpaceAfterConstructor": false,
    "typescript.format.insertSpaceAfterSemicolonInForStatements": true,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": true,
    "typescript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
    "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
    "typescript.format.insertSpaceAfterTypeAssertion": false,
    "typescript.preferences.importModuleSpecifier": "shortest",
    "javascript.preferences.importModuleSpecifier": "shortest",
    "typescript.reportStyleChecksAsWarnings": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
    "typescript.preferences.importModuleSpecifierEnding": "js",

    "go.toolsEnvVars": {
        "GOPROXY": "direct"
    },

    "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.server.path": null,
    "rust-analyzer.checkOnSave.enable": true,
    "[rust]": {
        "editor.rulers": [
            100
        ]
    },
    "rust-analyzer.imports.merge.glob": false,
    "rust-analyzer.imports.prefix": "plain",
    "rust-analyzer.cargo.buildScripts.useRustcWrapper": true,
    "rust-analyzer.cargo.features": [
        "all"
    ],

    "editor.semanticTokenColorCustomizations": {
        "[Gruvbox Dark Medium]": {
            "rules": {
                "selfKeyword": {
                    "bold": true,
                    "foreground": "#fe8019"
                },
                "selfParameter": {
                    "bold": true,
                    "foreground": "#fe8019"
                },
                "namespace": {
                    "foreground": "#D3869B"
                },
                "enumMember": {
                    "foreground": "#8EC07C",
                }
            }
        },
        "rules": {
            "method": {
                "bold": true
            },
            "enumMember": {
                "bold": true
            },
            "*.mutable": {
                "underline": false
            },
        },
    },
}